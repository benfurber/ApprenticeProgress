{
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "components": "./app/components",
          "mocks": "./app/__mocks__",
          "queries": "./app/queries",
          "redux-modules": "./app/redux/modules",
          "redux-store": "./app/redux/store",
          "screens": "./app/screens",
          "styles": "./app/styles",
          "types": "./app/types",
          "utils": "./app/utils"
        }
      }
    ]
  ],
  "presets": ["react-native"]
}
